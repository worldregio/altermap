@article{begue_remote_2020,
	title = {Remote {Sensing} {Products} and {Services} in {Support} of {Agricultural} {Public} {Policies} in {Africa}: {Overview} and {Challenges}},
	volume = {4},
	issn = {2571-581X},
	shorttitle = {Remote {Sensing} {Products} and {Services} in {Support} of {Agricultural} {Public} {Policies} in {Africa}},
	url = {https://www.frontiersin.org/articles/10.3389/fsufs.2020.00058},
	abstract = {In the last decade, governments, international institutions, donors, the private sector, etc. have shown a renewed interest in agricultural issues in Sub-Saharan Africa (SSA). This interest came with a strong need for information in countries where the lack of reliable and timely basic information can be a problem. Thanks to its capacity to observe the Earth at local, regional, and global scales and from various vantage points, satellite remote sensing is a powerful tool to streamline the monitoring and improvement of the existing systems, and thus to support decision-making. However, the path from satellite images to public policy decisions is not straightforward, and today, only few operational information services are available in SSA countries (e.g., early warning systems for food security and desert locust plagues prevention, rangeland production forecasting). This paper aims to analyze the gap between the technical aspects of the remote sensing sciences and the pragmatic need for relevant data to address agricultural policies in Africa and produce operational recommendations. To achieve this goal, the authors (1) determine the information, and in particular the geoinformation, needed to develop, implement and evaluate agricultural public policies (2) illustrate the role of remote sensing as a public policy tool in SSA through an overview of the current off-the-shelf products and services derived from Earth Observation systems, and (3) propose an analysis of the existing gap between the remote sensing research community and the policy makers. Based on this review, the authors conclude that to benefit from this technological advancement and bridge the gap between technical analysts and policy makers, some key points are fundamental: capacity building, political will and institutional commitment, public-private partnership, and proofs of concept.},
	urldate = {2023-01-23},
	journal = {Frontiers in Sustainable Food Systems},
	author = {Bégué, Agnès and Leroux, Louise and Soumaré, Mamy and Faure, Jean-François and Diouf, Abdoul Aziz and Augusseau, Xavier and Touré, Labaly and Tonneau, Jean-Philippe},
	year = {2020},
	file = {Full Text PDF:C\:\\Users\\fr200\\Zotero\\storage\\AKHUI8JX\\Bégué et al. - 2020 - Remote Sensing Products and Services in Support of.pdf:application/pdf},
}

@book{bourgoin_atlas_2019,
	title = {Atlas des dynamiques observées dans le bassin de collecte de la {Laiterie} du {Berger}},
	isbn = {978-2-87614-745-4},
	abstract = {En Afrique de l'Ouest, l'élevage est resté jusqu'à récemment en marge de l'industrialisation des filières laitières. De grandes laiteries se sont implantées dans les capitales, notamment dans les pays côtiers, mais sans investir de manière ambitieuse dans la collecte de lait local et en utilisant surtout du lait en poudre importé. Pourtant, à côté de ces grandes industries, des entreprises de taille souvent plus modeste s' appuient au contraire sur la collecte de lait local. La Laiterie du Berger implanté à Richard-Toll au nord du Sénégal en est un cas exemplaire. Après dix ans d'activités, cet atlas présente un diagnostic de ce qu'il convient d'appeler une aventure humaine et économique que l'on retrouve dans le modèle de social business appliqué par la Laiterie du Berger et son emblématique Directeur Bagoré Bathily. Le contexte géographique et environnemental est présenté dans une première partie. Le système de collecte et son impact sur la productivité laitière et l'écologie pastorale sont décrits dans une seconde partie. Enfin, la troisième partie s' inscrit dans une vision plus prospective et amène le lecteur à s' interroger sur le social business. Cet atlas est le fruit d'une collaboration entre le Laiterie du Berger et la Recherche/Développement. Il bénéficie de l'implication de 26 auteurs et co-auteurs.},
	author = {Bourgoin, Jeremy and Corniaux, Christian and Touré, Labaly and Cesaro, Jean-Daniel},
	month = jan,
	year = {2019},
	file = {Full Text PDF:C\:\\Users\\fr200\\Zotero\\storage\\E2QSMPM4\\Bourgoin et al. - 2019 - Atlas des dynamiques observées dans le bassin de c.pdf:application/pdf},
}
